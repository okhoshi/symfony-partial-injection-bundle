{
    "name": "okhoshi/symfony-partial-injection-bundle",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "version": "0.0.1",
    "autoload": {
        "psr-4": {
            "Okhoshi\\PartialInjectionBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okhoshi\\PartialInjectionBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "symfony/http-kernel": "^v7.0",
        "symfony/dependency-injection": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    }
}
